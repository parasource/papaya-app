{
  "expo": {
    "name": "papaya",
    "slug": "papaya",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/img/logoPapaya.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#111111"
    },
    "scheme": "papaya",
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./assets/img/logoPapaya.png",
          "color": "#ffffff"
        }
      ]
    ],
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.parasource.papaya.ios"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#111111"
      },
      "useNextNotificationsApi": true
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
